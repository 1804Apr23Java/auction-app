<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
		"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
		<property name="hibernate.connection.username"></property>
		<property name="hibernate.connection.password"></property>
		<property name="hibernate.connection.url">jdbc:oracle:thin:@revaturedatabase5562.ctlwui5soyig.us-east-2.rds.amazonaws.com:1521:ORCL</property>
		<property name="hibernate.connection.driver_class">oracle.jdbc.driver.OracleDriver</property>
		<!-- this can be useful during development but very noisy in the console -->
		<property name="hibernate.show_sql">true</property>
		<!-- choose whether to create db or update existing ddl -->
		<!-- update by default -->
		<property name="hibernate.hbm2ddl.auto">create</property>
		<!-- locations of domain object mappings -->
		<mapping class="com.revature.beans.User"></mapping>
		<mapping class="com.revature.beans.Item"></mapping>
		<mapping class="com.revature.beans.BiddingHistory"></mapping>
		<mapping class="com.revature.beans.ItemTransaction"></mapping>

	</session-factory>
</hibernate-configuration>